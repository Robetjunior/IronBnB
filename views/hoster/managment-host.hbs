  {{#if userInSession}}
<div class="container">
    <div class="row">
        <div class="w-100 my-md-3 pl-md-3">
            <div class="profile bg-info mr-md-3 pt-3 px-3 pt-md-5 px-md-5 text-center text-white overflow-hidden">
                <h2>Welcome, {{ userInSession.username }}!</h2>
                <a href="/userProfile"><img class="bd-placeholder-img rounded-circle" width="300" height="300" src="images/profile.png"></a>
                    <title>Placeholder</title>
                    <react width="100%" height="100%" fill="#777"></react>
                    <text x="50%" y="50%" fill="#777" dy=".3em"></text>
                </img>
                <h4><strong>Email: {{userInSession.email}}</strong> </h4>
            </div>
        </div>


 {{#if findHost}}
 <div class="col-md-6">
     {{#each findHost}}
        {{#if reservados}}
        <div class=" card card-nav-tabs">
            <div class="card-header card-header-primary d-flex justify-content-center  ">
                <h2 class="display-5">Reservado</h2>
                <div class="card-body">
                    <div class="tab-content"> 
                        <img class="card-img-top" src="{{this.imgPath}}" alt="host" style="height: 225px; width: 100%; display: block;" data-holder-rendered="true">
                        <h5><strong>Titulo:</strong> {{this.title}}</h5>
                        <h5><strong>Localização:</strong> {{this.local}}</h5>
                        <h5 class="card-text"><strong>Quantidade de hospedes:</strong>  {{this.qntHosp}}</h5>
                                            <h5><strong>Price: </strong> R${{this.preco}},00</h5>
                    </div>
                </div>
                
                {{else}}
                 <h2 class="display-5">Não Reservado</h2>
                <div class="card-body">
                    <div class="tab-content"> 
                        <img class="card-img-top" src="{{this.imgPath}}" alt="host" style="height: 225px; width: 100%; display: block;" data-holder-rendered="true">
                        <h5><strong>Titulo:</strong> {{this.title}}</h5>
                        <h5><strong>Localização:</strong> {{this.local}}</h5>
                        <h5 class="card-text"><strong>Quantidade de hospedes:</strong>  {{this.qntHosp}}</h5>
                        <h5><strong>Price: </strong> R${{preco}},00</h5>
                </div>
               
                {{/if}}
            </div>
            {{/each}}
        {{else}}
        <h2 class="text-center m-auto ">Você ainda não possui um host cadastrado. </h2>
        <div class="jumbotron  d-flex justify-content-center" style="background-color: transparent!important;"> <div class="jumbotron  d-flex justify-content-center" style="background-color: transparent!important;"><div class="   d-flex justify-content-center text-center h-100">
<a class="btn padrao  w-75 mb-auto" href="/host/create" ><svg width="10%" height="10%" viewBox="0 0 16 16" class="bi bi-file-earmark-diff-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
  <path fill-rule="evenodd" d="M2 3a2 2 0 0 1 2-2h5.293a1 1 0 0 1 .707.293L13.707 5a1 1 0 0 1 .293.707V13a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V3zm7 2V2l4 4h-3a1 1 0 0 1-1-1zM8 6a.5.5 0 0 1 .5.5V8H10a.5.5 0 0 1 0 1H8.5v1.5a.5.5 0 0 1-1 0V9H6a.5.5 0 0 1 0-1h1.5V6.5A.5.5 0 0 1 8 6zm-2.5 6.5A.5.5 0 0 1 6 12h4a.5.5 0 0 1 0 1H6a.5.5 0 0 1-.5-.5z"/>
</svg> Create your host </a>
    </div></div></div>
    </div>
    {{/if}}
                </div>
            </div>
        </div>
        



    </div>
</div>

{{else}}
    <p>Still no logged in user, sorry!</p>
{{/if}}
  
  
  
