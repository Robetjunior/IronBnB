                            </div>
        {{#if findHost}}
 
            <div class="section w-100">
                <div class="container-fluid ">
                    {{#if reservado}}
                        <div role="main" class="inner cover text-center m-auto d-flex justify-content-center" style="width:200px;">
                            <div id="nav-tabs row  d-flex justify-content-center "> 
                                <h2 class="display-5">Meus Hosts</h2>
                            </div>
                            <div class=" d-flex justify-content-center">
                                <p class="lead"> TesteTesteTesteTesteTeste</p>
                            </div>
                            
                        {{else}}
                        <div class="my-3 py-3 mt-auto d-flex justify-content-center ">
                            <h1 class="title cover-heading">YOU HAVE NO HOST HERE.</h1>
                            
                        </div>
                    {{/if}}
                   
